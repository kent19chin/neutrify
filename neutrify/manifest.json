{
  "name": "Neutrify",
  "version": "0.0.1",
  "description": "Track carbon footprints while saving the world!",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.jpeg"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "notes.css"
      ],
      "js": [
        "notes.js"
      ]
    }
  ],
  "manifest_version": 2
}